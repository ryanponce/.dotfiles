{
  "language_models": {
    "anthropic": {
      "version": "1"
    }
  },
  "vim_mode": true,
  "soft_wrap": "editor_width",
  "buffer_font_family": "Berkeley Mono",
  "ui_font_family": "Berkeley Mono",
  "ui_font_size": 12,
  "buffer_font_size": 12,
  "buffer_font_features": {
    "calt": true,
    "ss01": true,
    "ss02": true,
    "ss03": true,
    "ss04": true,
    "ss05": true,
    "ss06": true,
    "ss07": true,
    "ss08": true,
    "ss09": true,
    "liga": true
  },
  "project_panel": {
    "dock": "left"
  },
  "theme": {
    "mode": "system",
    "light": "Github Light",
    "dark": "Github Light"
  },
  "assistant": {
    "default_model": {
      "provider": "anthropic",
      "model": "claude-3-5-sonnet-latest"
    },
    "dock": "right",
    "version": "2",
    "provider": null
  },
  "format_on_save": "on",
  "languages": {
    "TSX": {
      "formatter": "prettier",
      "code_actions_on_format": {
        "source.fixAll.eslint": true
      }
    },
    "TypeScript": {
      "formatter": "prettier",
      "code_actions_on_format": {
        "source.fixAll.eslint": true
      }
    }
  }
}
